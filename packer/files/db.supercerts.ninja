;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	supercerts.ninja. root.supercerts.ninja. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.supercerts.ninja.
@	IN	A	{{SET_IP}}
@	IN	AAAA	::1
ns  IN  A   {{SET_IP}}
ssl IN  A {{SET_IP}}
